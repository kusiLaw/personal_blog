<div class = 'login-wraper c-flex'>
  <div class='login-form c-flex'>
     <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
       
       <div class="field">
         <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: 'Email', class: 'login-input' %>
       </div>

       <div class="field">
         <%= f.password_field :password, autocomplete: "current-password", placeholder: 'Password', class: 'login-input' %>
       </div>

       <div class="actions">
         <%= f.submit "Log in" ,class: 'login-input' %>
       </div>
     <% end %>
  </div>
  <div class= 'login-options flex'>
     <%= render "devise/shared/links"  %>
  </div>
</div>